  77          u8 *int_to_str( u8 *str,u16 num)
  78          {
  79   1        u8 *p;
  80   1        str[0] = num/10000+0x30;
  81   1        str[1] = num%10000/1000+0x30;
  82   1        str[2] = num%1000/100+0x30;
